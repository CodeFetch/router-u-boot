# =============
# CLEAN TARGETS
# =============

clean:
	@$(call echo_green,Making $@...)
	@find $(obj) -type f \
		\( -name 'core' -o -name '*.bak' -o -name '*~' \
		-o -name '*.o'  -o -name '*.a' -o -name .depend \) -print \
		| xargs rm -f
	@rm -f $(obj)tools/mkimage tools/envcrc
	@rm -f $(obj)lib_bootstrap/*.o
	@rm -f $(obj)lib_bootstrap/*.a
	@rm -f $(obj)bootstrap $(obj)bootstrap.bin $(obj)tuboot.bin $(obj)u-boot.lzimg $(obj)u-boot.bin.lzma $(obj)bootstrap.map

lzma_host_clean:
	@$(call echo_green,Removing lzma host utility...)
	@rm -fr $(LZMA_DIR)

clobber: clean
	@$(call echo_green,Making $@...)
	@find $(obj) -type f \( -name '*.d' \
		-o -name '*.srec' -o -name '*.bin' -o -name u-boot.img \) \
		-print0 \
		| xargs -0 rm -f
	@rm -f $(OBJS) $(obj)*.bak $(obj)include/version_autogenerated.h
	@rm -f $(obj)include/config.h $(obj)include/config.mk
	@rm -fr $(obj)*.*~
	@rm -f $(obj)u-boot $(obj)u-boot.map $(obj)u-boot.hex $(ALL)
	@rm -f $(obj)tools/crc32.c $(obj)tools/environment.c
	@rm -f $(obj)include/asm/proc $(obj)include/asm/arch $(obj)include/asm

distclean: clobber
